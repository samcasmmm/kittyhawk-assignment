{
   "name": "todo-backend-ts",
   "version": "1.0.0",
   "private": true,
   "main": "dist/index.js",
   "scripts": {
      "dev": "tsc-watch --onSuccess \"node ./dist/index.js\"",
      "build": "tsc -p tsconfig.json",
      "start": "node -r module-alias/register dist/index.js",
      "lint": "eslint . --ext .ts",
      "prepare": "husky install"
   },
   "dependencies": {
      "bcryptjs": "^3.0.3",
      "cors": "^2.8.5",
      "dotenv": "^16.6.1",
      "express": "^4.21.2",
      "helmet": "^8.1.0",
      "husky": "^9.1.7",
      "jsonwebtoken": "^9.0.2",
      "module-alias": "^2.2.3",
      "mongoose": "^7.8.7",
      "morgan": "^1.10.1"
   },
   "devDependencies": {
      "@types/bcryptjs": "^3.0.0",
      "@types/cors": "^2.8.19",
      "@types/express": "^4.17.25",
      "@types/jsonwebtoken": "^9.0.10",
      "@types/morgan": "^1.9.10",
      "@types/node": "^20.19.25",
      "@typescript-eslint/eslint-plugin": "^6.21.0",
      "@typescript-eslint/parser": "^6.21.0",
      "eslint": "^8.57.1",
      "ts-node-dev": "^2.0.0",
      "tsc-watch": "^7.2.0",
      "typescript": "^5.9.3"
   },
   "_moduleAliases": {
      "@/config": "dist/config",
      "@/controllers": "dist/controllers",
      "@/models": "dist/models",
      "@/routes": "dist/routes",
      "@/middleware": "dist/middleware",
      "@/utils": "dist/utils"
   }
}